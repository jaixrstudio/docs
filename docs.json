{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "name": "Summer Host",
  "colors": {
    "primary": "#615ffe",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "pages": [
          "index.mdx"
        ],
        "groups": [
          {
            "group": "Individual",
            "pages": [
              "Individual/matt"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/deploy",
          "icon": "discord"
        },
        {
          "anchor": "Github",
          "href": "https://www.google.com",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Discord",
        "href": "https://discord.gg/deploy"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Github",
      "href": "https://www.google.com"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/deploy",
      "github": "https://www.google.com"
    }
  },
  "fonts": {
    "heading": {
      "family": "Cormorant Garamond",
      "weight": 700
    },
    "body": {
      "family": "Inter",
      "weight": 400
    }
  }
}
